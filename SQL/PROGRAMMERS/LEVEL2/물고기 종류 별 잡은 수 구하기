// 방법1
SELECT COUNT(I.FISH_TYPE) as FISH_COUNT, (SELECT FISH_NAME FROM FISH_NAME_INFO WHERE FISH_TYPE = I.FISH_TYPE) as FISH_NAME
FROM FISH_INFO I
GROUP BY I.FISH_TYPE
ORDER BY FISH_COUNT DESC

// 방법2
SELECT COUNT(FISH_TYPE) as FISH_COUNT, FISH_NAME
FROM FISH_INFO JOIN FISH_NAME_INFO USING (FISH_TYPE)
GROUP BY FISH_NAME
ORDER BY FISH_COUNT DESC
